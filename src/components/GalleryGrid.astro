---
import cardMap from './card-map.js';
---

<div
  id="gallery"
  class="grid grid-cols-6 gap-x-4 gap-y-8 sm:gap-x-12 sm:gap-y-10 lg:gap-y-16 px-4 sm:px-8 py-12"
>
  {cardMap.map(({ Component, typeClass, size }) => (
    <Component typeClass={typeClass} size={size} />
  ))}
</div>


<style>
  .auto-rows-auto {
    grid-auto-rows: 1fr;
  }
</style>