---
import CardLabel from "../Card-label.astro";
import Card from "../Card.astro";
import { Image } from "astro:assets";

import myImage from "../../images/homepage/deliberate.gif";

const title = "Deliberate AI";
const description = "asdf";
const year = "2022";
const types = ["User Experience", "AI"];
const size = "small";
const hasVideo = false;
const alt =
  "Deliberate AI logo and screens from prototypes for the Deliberate AI health application.";
---

<Card size="small" href="#" title={title} types={types} year={year}>
  <Image src={myImage} alt={alt} />
</Card>

<style>
  img {
    width: 100%;
  }
</style>
