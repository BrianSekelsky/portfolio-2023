---
import Layout from "../../layouts/Layout.astro";

import ProjectHeader from "../../components/ProjectHeader.astro";
import SmallHeading from "../../components/SmallHeading.astro";
import ProjectGrid from "../../components/ProjectGrid.astro";
import { Image } from "astro:assets";

import webm from "../../images/homepage/concert-interface.webm";
import webmMp4 from "../../images/homepage/concert-interface.mp4";
import poster from "../../images/homepage/concert-interface-poster.jpg";
---

<Layout title="Brian Sekelsky">
  <main>
    <ProjectHeader
      title="DIY concert interface."
      objective="DIY concert interface."
